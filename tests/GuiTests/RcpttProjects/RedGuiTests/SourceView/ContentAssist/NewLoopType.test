	with [get-view Problems | get-tree] {
	    get-property "getItems().TreeItem[0].getItems().length" | equals 1 | verify-true
	    get-property "getItems().TreeItem[0].getItems().TreeItem[0].getText()" | equals "For loop contains no keywords" 
